<div id="boostscontainer">
  <form action="/boosts" method="POST">
    <b>Boosts:</b>         
    <input hidden type="text" name='taskid' value="<%=selectTask.id%>">
    <input type="text" name='boost' required> 
    <input type="submit" value="add" class='btnlink'>  
  </form>
  <div id="boost">
    <%selectTask.boosts.forEach(function(boost){%>
      <a href="/boosts/remove?boostid=<%=boost.id%>&taskid=<%=selectTask.id%>" class='trackertabs boosttabs'><%=boost.name%></a> 
    <%})%>
  </div>
</div>